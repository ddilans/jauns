mkdir build
cd build
../configure --target=x86_64-jauns-linux --prefix=/mnt/jauns/pkgs/gcc-10.2.0 --with-glibc-version=2.33 --with-sysroot=/mnt/jauns --with-newlib --without-headers --enable-initfini-array --disable-nls --disable-shared --disable-decimal-float --disable-threads --disable-libatomic --disable-libgomp --disable-libquadmath --disable-libssp --disable-libvtv --disable-libstdcxx --enable-languages=c,c++
make
